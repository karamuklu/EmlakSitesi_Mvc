<body class="home page page-template">
    <div class="top">
        <div class="container">
            <div class="top-inner inverted">
                <div class="header clearfix">
                    <div class="branding pull-left">
                        <div class="logo">
                            <a href="~/Anasayfa/Index" title="Home">
                                <img src="~/properta/assets/img/logo.png" alt="Home">
                            </a>
                        </div>
                        <!-- /.logo -->

                        <div class="site-name">
                            <a href="~/Anasayfa/Index" title="Home" class="brand">
                                Yeni Nesil Emlak Sitesi
                            </a>
                        </div>
                        <!-- /.site-name -->
                    </div>

                    <div class="contact-top">
                        <ul class="menu nav">
                            <li class="first leaf facebook"><a href="http://www.facebook.com" class="facebook"><i>F</i></a></li>
                            <li class="leaf flickr"><a href="http://www.flickr.com" class="flickr"><i>n</i></a></li>
                            <li class="leaf google-plus"><a href="http://plus.google.com" class="google"><i>g</i></a></li>
                            <li class="leaf linkedin"><a href="http://linkedin.com" class="linkedin"><i>l</i></a></li>
                            <li class="leaf twitter"><a href="http://www.twitter.com" class="twitter"><i>T</i></a></li>
                            <li class="last leaf vimeo"><a href="http://vimeo.com" class="vimeo"><i>v</i></a></li>
                        </ul>
                    </div>

                    <div class="user-area pull-right">
                        <div class="menu-anonymous-container">
                            <ul id="menu-anonymous" class="nav nav-pills">
                                <li class="menu-item"><a href="~/properta/register">Register</a></li>
                                <li class="menu-item">

                                    @if (Session["Kullanıcı"] != null)
                                    {
                                        <a href="/Emlak/Index">
                                           @Session["Kullanıcı"].ToString()
                                        </a>
                                    }
                                    else
                                    {
                                        <a href="/Login/Login">
                                            Giriş Yap
                                        </a>
                                    }


                                </li>
                            </ul>
                        </div>

                    </div>
                    <!-- /.user-area -->
                </div>
                <!-- /.header -->
                <div class="navigation navbar clearfix">
                    <div class="pull-left">
                        <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>

                        <div class="nav-collapse collapse">
                            <ul id="menu-main" class="nav">
                                

                                <li class="menu-item menu-item-parent">
                                    <a href="~/Anasayfa/EmlakListesi">Emlak Listesi</a>

                                    <ul class="sub-menu">
                                        <li class="menu-item"><a href="~/Anasayfa/EmlakListesi">Properties listing</a></li>
                                        <li class="menu-item"><a href="~/properta/properties/property-detail">Property detail</a></li>

                                        <li class="menu-item"><a href="~/properta/agencies">Agencies listing</a></li>
                                        <li class="menu-item"><a href="~/properta/agencies/agency-detail">Agencies detail</a></li>

                                        <li class="menu-item"><a href="~/properta/agents">Agents listing</a></li>
                                        <li class="menu-item"><a href="~/properta/agents/agent-detail">Agent detail</a></li>

                                        <li class="menu-item"><a href="~/properta/register">Register</a></li>
                                        <li class="menu-item"><a href="~/properta/login">Login</a></li>
                                    </ul>
                                </li>

                                <li class="menu-item menu-item-parent">
                                    <a href="#">Alt Sayfalar</a>
                                    <ul class="sub-menu">
                                       @* <li class="menu-item"><a href="~/properta/404">404 page</a></li>*@
                                        <li class="menu-item"><a href="~/Anasayfa/CokSorulanlar">Sık Sorulan Sorular</a></li>
                                        @*<li class="menu-item"><a href="~/properta/pricing">Pricing</a></li>*@
                                    </ul>
                                </li>

                                <li class="menu-item">
                                    <a href="~/Anasayfa/iletisim">İletişim</a>
                                </li>
                            </ul>
                        </div>
                    </div>

                    <div class="pull-right">
                        <div class="list-property">
                            <a href="/submissions">
                                List your property<div class="ribbon"><span class="icon icon-normal-circle-plus"></span></div>
                            </a>
                        </div>
                        <!-- /.list-property -->
                    </div>
                    <!-- /.pull-right -->
                </div>

                <div class="breadcrumb pull-left">
                    <!-- Breadcrumb NavXT 4.4.0 -->
                    <a title="Go to Properta." href="~/properta/" class="home">Properta</a> &gt; Page
                </div>

                <!-- /.breadcrumb -->
            </div>
        </div>
    </div>

    @RenderBody()

